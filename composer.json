{
    "name": "yosymfony/spress-plugin-dataloader",
    "type": "spress-plugin",
    "description": "Dataloader plugin for Spress",
    "keywords": ["spress", "plugin", "json"],
    "license": "MIT",
    "require": {
        "yosymfony/spress-installer": "1.0.*@dev"
    },
    "extra": {
        "spress_name": "Spress-plugin-dataloader",
        "spress_class": "SpressPlugins\\SpressDataLoader\\SpressDataLoader",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SpressPlugins\\SpressDataLoader\\": ""
        }
    },
    "minimum-stability": "dev"
}